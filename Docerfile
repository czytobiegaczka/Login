FROM node:latest
WORKDIR /app
COPY package.json .
RUN npm install
COPY index.js .
COPY biegi.html .
COPY index.html .
COPY month.html .
COPY protected.html .
COPY scripts.js .
COPY sha256.js .
COPY style.css .
CMD ["node","index.js","biegi.html","index.html","month.html","protected.html","scripts.js","sha256.js","style.css"]